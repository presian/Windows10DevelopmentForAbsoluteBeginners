<Page
    x:Class="_45_AdeptlyAdaptiveChallenge.FoodPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:_45_AdeptlyAdaptiveChallenge"
    xmlns:tepmlates="using:_45_AdeptlyAdaptiveChallenge.Templates"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:data ="using:_45_AdeptlyAdaptiveChallenge.DataModels"
    mc:Ignorable="d">

    <Grid Background="LightGray">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <tepmlates:NewsItemListDataTemplate News="{x:Bind News, Mode=OneWay}"/>
    </Grid>
</Page>































